<?xml version="1.0"?>

<PropertyList>
	
	<path>amps.ac</path>
	
	<animation>
		<type>material</type>
		<object-name>amps.base</object-name>
		<object-name>amps.needle</object-name>
		<emission>
			<red>1.0</red>
			<green>0.2</green>
			<blue>0.0</blue>
			<factor-prop>/systems/electrical/outputs/instrument-lights-norm</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>amps.needle</object-name>
		<property>systems/electrical/amps</property>
		<interpolation>
			<entry>
				<ind>-20</ind>
				<dep>-38.66</dep>
			</entry>
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>20</ind>
				<dep>38.66</dep>
			</entry>
		</interpolation>
		<axis>
			<x>-1.0</x>
			<y>0.0</y>
			<z>0.0</z>
		</axis>
	</animation>
	
</PropertyList>
