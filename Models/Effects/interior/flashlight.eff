<?xml version="1.0" encoding="utf-8"?>
<!-- ported from C150 by Harald Johnsen et.al, thanks! -->
<PropertyList>
	<name>flashlight</name>
	<inherits-from>Effects/model-interior</inherits-from>
	<parameters>
		<texture n="4">
			<type>cubemap</type>
			<images>
				<positive-x>Aircraft/c140/Models/Effects/white.png</positive-x>
				<negative-x>Aircraft/c140/Models/Effects/white.png</negative-x>
				<positive-y>Aircraft/c140/Models/Effects/white.png</positive-y>
				<negative-y>Aircraft/c140/Models/Effects/white.png</negative-y>
				<positive-z>Aircraft/c140/Models/Effects/white.png</positive-z>
				<negative-z>Aircraft/c140/Models/Effects/white.png</negative-z>
			</images>
		</texture>
		<light-filter-one type="vec3d">0.5 0.5 0.5</light-filter-one>
		<light-filter-two type="vec3d">0.9 0.2 0.2</light-filter-two>
		<light-radius type="float">13</light-radius>
	</parameters>
</PropertyList>
