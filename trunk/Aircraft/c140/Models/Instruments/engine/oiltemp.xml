<?xml version="1.0"?>

<PropertyList>
	
	<path>oiltemp.ac</path>
	
	<animation>
		<type>material</type>
		<object-name>ot.base</object-name>
		<object-name>ot.needle</object-name>
		<emission>
			<red>1.0</red>
			<green>0.2</green>
			<blue>0.0</blue>
			<factor-prop>/systems/electrical/outputs/instrument-lights-norm</factor-prop>
		</emission>
	</animation>
	
	
	<animation>
		<type>rotate</type>
		<object-name>ot.needle</object-name>
		<property>/engines/engine[0]/oil-temperature-degf</property>
		<interpolation>
			<entry>
				<ind>100</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>300</ind>
				<dep>232.32</dep>
			</entry>
		</interpolation>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	
</PropertyList>


