<piston_engine name="C-85">
	<minmp unit="INHG">         10.0 </minmp>
	<maxmp unit="INHG">         28.5 </maxmp>
	<displacement unit="IN3"> 188 </displacement>
	<maxhp>        85 </maxhp>
	<cycles>         4.0 </cycles>
	<idlerpm>      700.0 </idlerpm>
	<maxrpm>      2800.0 </maxrpm>
